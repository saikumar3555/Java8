package in.ashokit;

import java.util.stream.Stream;

public class Demo {
	
	public static void main(String[] args) {
		System.out.println("-----------------------------Serial Stream------------------");
	
		Stream.of(1,2,3,4,5,6,7)
		      .forEach(num -> System.out.println(num + " "+ Thread.currentThread().getName()));
	}
	

}
